unit CFX.RegisterClass;

interface
  uses Classes,
       CFX.Button, CFX.Checkbox,;


procedure Register;

implementation

procedure Register;
begin
    RegisterComponents(REGISTER_CLASS_NAME,[FXMinimisePanel, FXPanel]);
    RegisterComponents('Cods Fluent UX', [FXButton]);
    RegisterComponents('Cods Fluent UX', [FXCheckBox]);
end;

end.
